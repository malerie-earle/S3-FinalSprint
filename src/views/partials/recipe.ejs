<div class="recipeResult">
  <h3><a href="/recipe/id/<%= Recipes._id %>"><%= Recipes.title %></a></h3>
  <p><b>ID:</b></p> <%= Recipes._id %>
  <% Recipes.ingredients.forEach(function(ingredient) { %>
    <p><%= ingredient %></p>
  <% }); %>
  <a href="/recipe/id/<%= Recipes._id %>">See More >></a>
</div>